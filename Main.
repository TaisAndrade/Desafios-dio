package desafio_dio;

public class Main {

  public static void main(String[] args) {

   Cliente tais = new Cliente();
   tais.setNome("Tais");

   Conta cc = new ContaCorrente(tais);
   Conta poupanca = new ContaPoupanca(venilton);

     cc.depositar(100);
     cc.transferir(100, poupanca);
     
     cc.imprimirExtrato();
     poupanca.imprimirExtrato();
	}

  }

}
